%------------------------------------------------------------------------------

tff(bool_type,type,(
    bool: $tType )).

tff(xx_type,type,(
    xx: $int )).

tff(xy_type,type,(
    xy: $int )).

tff(c_type,type,(
    c: ( $int * $int ) > $o )).

tff(axiom1_axiom,axiom,(
    (xx = 0) )).

tff(axiom2_axiom,axiom,(
    (xy = 0) )).

