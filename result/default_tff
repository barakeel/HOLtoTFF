tff(bool_type,type,(
    bool: $tType )).

tff(xx_type,type,(
    xx: $int )).

tff(c_type,type,(
    c: ( $int * $int ) > $o )).

tff(c0_type,type,(
    c0: ( $int * $int ) > $o )).

tff(c01_type,type,(
    c01: ( $int * $int ) > $int )).

tff(axiom1_axiom,axiom,(
    ( $lesseq(0,xx) & ( $lesseq(0,$sum(xx,1)) & ( ( xx = 2 ) & ( ~ ( $sum(xx,1) = 3 ) ) ) ) ) )).